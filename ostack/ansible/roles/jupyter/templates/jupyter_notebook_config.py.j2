c.NotebookApp.open_browser = {{ jupyter_open_browser }}
c.NotebookApp.ip = '{{ jupyter_ip }}'
c.NotebookApp.port = {{ jupyter_port }}
c.NotebookApp.password = '{{ jupyter_password }}'
c.GatewayClient.url = http:{{machine_name}}:{{ jupyter_port }}
c.NotebookApp.tornado_settings = { 'headers': {
        'Content-Security-Policy':  "default-src https: 'unsafe-inline'; connect-src https: wss:"
    }
}
